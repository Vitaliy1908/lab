}

public class MainClass {
    public static void main(String[] args) {
        One oneInstance = new One(1.5, 10, 2.3);
        Two twoInstance = new Two(5, 3.7, 8);
        Three threeInstance = new Three(4.2, 15, 6.8);

        oneInstance.voidMethod();
        double result1 = oneInstance.typedMethod(5);
        System.out.println("Результат typedMethod в класі One: " + result1);
        oneInstance.otherClassMethod(twoInstance);
        oneInstance.dynamicArgumentsMethod("Hello", "World");

        System.out.println();

        twoInstance.voidMethod();
        int result2 = twoInstance.typedMethod(3.5);
        System.out.println("Результат typedMethod в класі Two: " + result2);
        twoInstance.otherClassMethod(threeInstance);
        twoInstance.dynamicArgumentsMethod(1, 2, 3, 4, 5);

        System.out.println();

        threeInstance.voidMethod();
        int result3 = threeInstance.typedMethod(2.7);
        System.out.println("Результат typedMethod в класі Three: " + result3);
        threeInstance.otherClassMethod(oneInstance);
        threeInstance.dynamicArgumentsMethod("Hello", "World", "Java");
    }
}


